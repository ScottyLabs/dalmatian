{
    "name": "dalmatian",
    "module": "src/index.ts",
    "type": "module",
    "private": true,
    "devDependencies": {
        "@biomejs/biome": "^2.3.0",
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.0.0",
        "@types/bun": "latest",
        "@tsconfig/node24": "^24.0.1",
        "drizzle-kit": "beta",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.6",
        "oxlint": "^1.24.0",
        "oxlint-tsgolint": "0.3.0",
        "typescript": "^5.9.3"
    },
    "license": "(MIT OR Apache-2.0)",
    "scripts": {
        "start": "bun run src/index.ts",
        "lint": "oxlint --type-aware",
